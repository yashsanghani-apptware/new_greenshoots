# URL for the Event Registry service (use localhost as it will be run in Docker)
EVENT_REGISTRY_URL=http://event-registry:3000/publish

# MongoDB URL pointing to the host machine
MONGO_URL=mongodb://host.docker.internal:27017

# RabbitMQ URL pointing to the host machine
AMQP_URL=amqp://host.docker.internal:5672

# Port for the Event Registry service
PORT=3000

# Default locale for i18n
LOCALE=en

